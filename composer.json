{
	"name" : "wabel/zoho-crm-orm",
	"description" : "An ORM like package used to manipulate data from your Zoho CRM Account through the API.",
	"type" : "library",
	"homepage" : "http://mouf-php.com",
	"keywords" : [
	  	"Zoho CRM",
		"ZohoCRM",
		"Zoho",
		"ORM"
	],
	"require" : {
		"php" : ">=5.4",
		"guzzlehttp/guzzle": "~6.0",
	    "gossi/php-code-generator" : "dev-master",
	    "symfony/options-resolver": "~2.6.0",
        "psr/log": "~1.0"
	},
    "require-dev" : {
	  "phpunit/phpunit": "~4.0",
	  "satooshi/php-coveralls": "~1.0"
	},
	"autoload" : {
		"psr-4" : {
			"Wabel\\Zoho\\CRM\\" : "src"
		}
	},
    "autoload-dev" : {
  	    "psr-4" : {
		    "Wabel\\Zoho\\CRM\\" : "tests"
	    }
    }
}
